    <div class="settings-pane-inner">
        <div class="row">
            <div class="col-md-4">
                <div class="user-info">
                    <div class="user-image">
                        <a href="extra-profile.html">
                            <img src="{{ constant('STYLE_IMAGES') }}/user-2.png" class="img-responsive img-circle"/>
                        </a>
                    </div>

                    <div class="user-details">

                        <h3>
                            <a href="extra-profile.html">{{ input.cookie('fullname') }}</a>

                            <!-- Available statuses: is-online, is-idle, is-busy and is-offline -->
